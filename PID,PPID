#include <stdio.h>
#include <unistd.h>
#include <sys/types.h>

int main() {
    pid_t pid;

    pid = fork();  // create child process

    if (pid < 0) {
        printf("Fork failed\n");
    } 
    else if (pid == 0) {
        // Child process
        printf("Child Process: PID = %d, PPID = %d\n", getpid(), getppid());
    } 
    else {
        // Parent process
        printf("Parent Process: PID = %d, PPID = %d\n", getpid(), getppid());
    }

    return 0;
}
