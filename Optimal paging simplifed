#include <stdio.h>
int predict(int pages[],int frame[],int n,int index,int m){
    int i,j,farthest=index,res=-1;
    for(i=0;i<n;i++){
        for(j=index;j<m;j++) if(frame[i]==pages[j]){ if(j>farthest){ farthest=j; res=i; } break; }
        if(j==m) return i;
    }
    return (res==-1)?0:res;
}
int main(){
    int n=3, pages[]={7,0,1,2,0,3,0,4,2,3,0,3}, m=12;
    int frame[3],i,j,faults=0,filled=0;
    for(i=0;i<n;i++) frame[i]=-1;

    for(i=0;i<m;i++){
        int hit=0;
        for(j=0;j<n;j++) if(frame[j]==pages[i]) hit=1;
        if(!hit){
            if(filled<n) frame[filled++]=pages[i];
            else frame[predict(pages,frame,n,i+1,m)]=pages[i];
            faults++;
        }
        printf("Ref %d: ",pages[i]);
        for(j=0;j<n;j++) printf("%d ",frame[j]);
        printf("\n");
    }
    printf("Total Page Faults = %d\n",faults);
}
